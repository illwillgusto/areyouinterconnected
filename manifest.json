{
    "name": "Are You Interconnected",
    "version": "1.0",
    "manifest_version": 3,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "images.png"
    },
    "icons": {
        "128": "images.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "bookmarks"
    ],
    "optional_permissions": [
        "unlimitedStorage"
    ]
}